{
    "name": "Pip packages Download",
    "id": "pip",
    "version": "0.0.1",
    "description": "Installs pip packages",
    "privileged": true,
    "options": {
        "PACKAGES": {
            "type": "string",
            "description": "package-name",
            "default": "jupyter"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils",
        "ghcr.io/devcontainers/features/python"
    ]
}
